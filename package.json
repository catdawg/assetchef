{
  "name": "assetchef",
  "version": "0.0.2",
  "description": "Process assets using a configurable pipeline",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --forceExit --coverage --verbose -i",
    "codacy": "jest --forceExit --coverage --verbose && cat ./coverage/lcov.info | codacy-coverage",
    "dist": "tsc --p tsconfig.dist.json"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/catdawg/assetchef.git"
  },
  "keywords": [
    "asset",
    "processor",
    "chef",
    "cook",
    "atlas",
    "spritesheet",
    "game",
    "gamedev"
  ],
  "author": "Rui Campos <ruir2c@gmail.com> (http://github.com/catdawg)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/catdawg/assetchef/issues"
  },
  "homepage": "https://github.com/catdawg/assetchef#readme",
  "engines": {
    "node": ">=7.9.0"
  },
  "dependencies": {
    "ajv": "^6.2.1",
    "chokidar": "^2.0.4",
    "sane": "^4.0.2",
    "fnv-plus": "^1.2.12",
    "fs-extra": "^4.0.3",
    "npm": "^6.4.1",
    "verror": "^1.10.0",
    "winston": "^3.1.0",
    "change-emitter": "^0.1.6"
  },
  "devDependencies": {
    "@types/npm": "^2.0.29",
    "@types/chai": "^4.1.2",
    "@types/chokidar": "^1.7.5",
    "@types/fs-extra": "^5.0.1",
    "@types/tmp": "^0.0.33",
    "@types/verror": "^1.10.3",
    "@types/sinon": "^5.0.1",
    "@types/jest": "^22.1.1",
    "@types/debug": "^0.0.30",
    "@types/change-emitter": "^0.1.2",
    "@types/chance": "1.0.0",
    "@types/sane": "^2.0.0",
    "assetchef-textsplitter": "file:test_plugins/assetchef-textsplitter",
    "assetchef-logchanges": "file:test_plugins/assetchef-logchanges",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "codacy-coverage": "^2.0.3",
    "sinon": "^4.1.2",
    "source-map-support": "^0.5.9",
    "tmp": "^0.0.33",
    "ts-node": "^7.0.1",
    "jest": "^22.1.4",
    "ts-jest": "^22.0.2",
    "tslint": "^5.11.0",
    "typescript": "^2.7.2",
    "chance": "^1.0.16",
    "randomfschanger": "^0.0.8"
  }
}
